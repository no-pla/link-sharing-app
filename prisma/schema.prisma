
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model User {
  id       String   @id @default(auto()) @map("_id") @db.ObjectId
  email    String
  password String
  accounts Accounts[]
}

model Accounts {
  id             String   @id @default(auto()) @map("_id") @db.ObjectId
  google         String
  youtube        String
  linkedIn       String
  facebook       String
  frontendMentor String
  user           User     @relation(fields: [userId], references: [id])
  userId         String
}